    "express": "^4.16.2",
    "formik": "^0.11.11",
    "mongoose": "^5.0.7",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "nodemon": "^1.15.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
